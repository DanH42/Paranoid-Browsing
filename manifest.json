{
  "manifest_version": 2,

  "name": "Paranoid Browsing",
  "description": "Confuse surveillers by randomly browsing the internet.",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
	"tabs",
	"webNavigation",
	"http://*/*"
  ],

  "background": {
	"scripts": ["event.js"],
	"persistent": true
  }
}
